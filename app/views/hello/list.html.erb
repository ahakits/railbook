<table>
  <tr>
    <th>ISBN</th>
    <th>Title</th>
    <th>Price</th>
    <th>Publish</th>
    <th>Published</th>
    <th>Download</th>
  </tr>
  <% @books.each do |book| %>
      <tr style="background-color: <%= cycle('#FFffFF', '#FFff99') %>">
        <td><%= book.isbn %></td>
        <td><%= book.title %></td>
        <td><%= book.price %></td>
        <td><%= book.publish %></td>
        <td><%= book.published %></td>
        <td><%= book.dl %></td>
      </tr>
  <% end %>
</table>
<%= debug(@books) %>

